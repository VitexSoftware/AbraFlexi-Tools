{
    "name": "vitexsoftware/flexibee-tools",
    "description": "Several FlexiBee Testing Tools.",
    "type": "library",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitex",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "bin": ["bin/fbget", "bin/fbput", "bin/fbcp","bin/upreport"],
    "require": {
        "spoje.net/flexibee": "dev-master",
        "fzaninotto/faker": "*"
    },
    "autoload": {
