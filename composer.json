{
    "name": "vitexsoftware/flexibee-testing-tools",
    "description": "Several FlexiBee Testing Tools.",
    "type": "library",
    "minimum-stability": "dev",

    "authors": [
        {
            "name": "Vitex",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "bin": ["src/fbget.php", "src/fbput.php"],
    "require": {
        "spoje.net/flexipeehp": "dev-master",
        "vitexsoftware/ease-framework": "dev-master",
        "fzaninotto/faker": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "FlexiPeeHP\\Bricks\\": "FlexiPeeHP/Bricks",
            "FlexiPeeHP\\ui\\": "FlexiPeeHP/ui"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ease\\": "vendor/vitexsoftware/ease-framework/tests/src/Ease",
            "Test\\FlexiPeeHP\\": "vendor/spoje.net/flexipeehp/tests/src/FlexiPeeHP",
            "Test\\": "testing/"
        }
    },
    "require-dev": {
        "codeception/codeception": "^2.3"
    }
}
